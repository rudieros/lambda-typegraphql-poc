{
  "defaultSeverity": "error",
  "extends": ["tslint:recommended"],
  "jsRules": {},
  "rules": {
    "ban-types": [true, ["Object", "Use {} instead."], ["String"]],
    "member-access": false,
    "no-any": false,
    "no-empty-interface": true,
    //"no-import-side-effect": [false, {"ignore-module": "(\\.html|\\.css)$"}],
    "no-inferrable-types": false,
    "no-magic-numbers": true,
    "no-namespace": true,
    "no-non-null-assertion": true,
    "no-parameter-reassignment": true,
    //"no-unnecessary-type-assertion": true,
    "only-arrow-functions": true,
    // "prefer-for-of": true,
    // "promise-function-async": true,
    "typedef": [
      true,
      "call-signature",
      "parameter",
      "member-variable-declaration",
      "arrow-call-signature"
    ],
    "typedef-whitespace": false,
    "await-promise": false,
    "ban-comma-operator": true,
    "ban": false,
    "curly": true,
    "forin": true,
    "function-constructor": true,
    "import-blacklist": [true, "aws-sdk", "lodash"],
    // "label-position": true,
    // "no-arg": true,
    "no-bitwise": true,
    "no-conditional-assignment": true,
    "no-console": [true, "error"],
    "no-construct": true,
    "no-debugger": true,
    "no-duplicate-super": true,
    "no-duplicate-switch-case": true,
    // "no-duplicate-variable": true,
    "no-dynamic-delete": true,
    "no-empty": [true, "allow-empty-catch", "allow-empty-functions"],
    "no-eval": true,
    "no-floating-promises": true,
    "no-for-in-array": true,
    "no-implicit-dependencies": true,
    //"no-inferred-empty-object-type": true,
    "no-invalid-template-strings": true,
    "no-invalid-this": true,
    "no-misused-new": true,
    "no-null-keyword": true,
    "no-object-literal-type-assertion": false,
    "no-return-await": true,
    "no-shadowed-variable": true,
    "no-sparse-arrays": true,
    "no-string-literal": true,
    "no-string-throw": true,
    "no-submodule-imports": false,
    "no-switch-case-fall-through": true,
    "no-this-assignment": true,
    "no-unbound-method": true,
    "no-unnecessary-class": ["allow-constructor-only"],
    "no-unsafe-any": true,
    "no-unsafe-finally": true,
    "no-unused-expression": true,
    "no-unused-variable": true,
    "no-use-before-declare": false,
    "no-var-keyword": true,
    "no-void-expression": false,
    "prefer-conditional-expression": false,
    "prefer-object-spread": true,
    "radix": false,
    "restrict-plus-operands": true,
    "strict-boolean-expressions": false,
    "strict-type-predicates": true,
    "switch-default": true,
    "triple-equals": [true, "allow-null-check", "allow-undefined-check"],
    "typeof-compare": true,
    "unnecessary-constructor": false,
    "use-default-type-parameter": true,
    "use-isnan": true,
    "cyclomatic-complexity": [true, 10],
    "deprecation": true,
    "eofline": true,
    "indent": [true, "spaces", 2],
    "linebreak-style": [true, "LF"],
    "max-classes-per-file": [true, 3, "exclude-class-expressions"],
    "max-file-line-count": [true, 300],
    "max-line-length": [
      true,
      {
        "limit": 120,
        "ignore-pattern": "^import |^export {(.*?)}|class [a-zA-Z]+ implements |"
      }
    ],
    "no-default-export": false,
    "no-default-import": false,
    "no-duplicate-imports": true,
    "no-mergeable-namespace": true,
    "no-require-imports": true,
    "object-literal-sort-keys": false,
    "prefer-const": [true, { "destructuring": "all" }],
    "prefer-readonly": false,
    "trailing-comma": [true, { "multiline": "always", "singleline": "never" }],
    "align": [true, "parameters", "statements"],
    "array-type": false,
    "arrow-parens": true,
    "arrow-return-shorthand": [true],
    "binary-expression-operand-order": true,
    "class-name": true,
    "comment-format": [
      true,
      "check-lowercase",
      "check-space",
      { "ignore-words": ["TODO"] }
    ],
    "comment-type": [true, "doc", "singleline"],
    "completed-docs": false,
    "encoding": true,
    "file-name-casing": [false, "camel-case"],
    "import-spacing": true,
    "increment-decrement": true,
    "interface-name": [false, "always-prefix"],
    "interface-over-type-literal": false,
    "match-default-export-name": false,
    "newline-before-return": true,
    "newline-per-chained-call": false,
    "new-parens": true,
    "no-angle-bracket-type-assertion": false,
    "no-boolean-literal-compare": true,
    "no-consecutive-blank-lines": true,
    "no-irregular-whitespace": true,
    "no-parameter-properties": false,
    "no-trailing-whitespace": [
      true,
      "ignore-blank-lines",
      "ignore-template-strings",
      "ignore-comments",
      "ignore-jsdoc"
    ],
    "no-unnecessary-callback-wrapper": false,
    "no-unnecessary-initializer": true,
    "no-unnecessary-qualifier": true,
    "number-literal-format": true,
    "object-literal-key-quotes": [true, "as-needed"],
    "object-literal-shorthand": false,
    "one-line": [false, "check-catch", "check-finally", "check-else"],
    "one-variable-per-declaration": true,
    "ordered-imports": false,
    "prefer-function-over-method": false,
    "prefer-method-signature": true,
    "prefer-switch": [true, { "min-cases": 2 }],
    "prefer-template": [true],
    "prefer-while": true,
    "quotemark": [true, "single", "jsx-double"],
    "return-undefined": true,
    "semicolon": [true, "never"],
    "space-before-function-paren": [
      true,
      { "anonymous": "never", "named": "never", "asyncArrow": "always" }
    ],
    "space-within-parens": true,
    "switch-final-break": true,
    "type-literal-delimiter": true,
    "unnecessary-bind": true,
    "variable-name": [
      true,
      "ban-keywords",
      "check-format",
      "allow-leading-underscore"
    ],
    "whitespace": [true, "check-branch", "check-operator", "check-typecast"]
  },
  "rulesDirectory": []
}
